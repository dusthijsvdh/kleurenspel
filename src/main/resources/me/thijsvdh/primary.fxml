<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="me.thijsvdh.PrimaryController">
    <children>
        <Label text="Primary View" />
        <Button fx:id="primaryButton" onAction="#switchToSecondary" text="Switch to Secondary View" />
      <Label text="Voer hier tekst in:" />
      <TextField fx:id="primaryTextfield" onKeyTyped="#textFieldUpdate" promptText="Voer hier tekst in:" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
